{
  "name": "repco-core",
  "version": "0.1.0",
  "license": "MIT OR Apache-2.0",
  "maintainers": [
    "repco contributors"
  ],
  "main": "dist/lib.js",
  "type": "module",
  "bin": {
    "repco": "./bin.js"
  },
  "scripts": {
    "cli": "node bin.js",
    "build": "run-s build:*",
    "build:tsc": "tsc --build",
    "build:bundle": "node scripts/bundle.js",
    "watch": "run-p watch:*",
    "watch:tsc": "tsc --build -w",
    "watch:bundle": "node scripts/bundle.js -w",
    "docs": "typedoc src/mod.ts --out ./docs",
    "test": "brittle dist/test/*.js",
    "build-test": "yarn build && yarn test"
  },
  "dependencies": {
    "@ipld/car": "^4.1.6",
    "@ipld/dag-cbor": "^7.0.3",
    "@multiformats/blake2": "^1.0.11",
    "@pkgjs/parseargs": "^0.11.0",
    "@types/cli-progress": "^3.11.0",
    "@ucans/ucans": "https://gitpkg.now.sh/Frando/ts-ucan/packages/ucans?fix-0.11.3",
    "casual-browserify": "^1.5.19-2",
    "cli-progress": "^3.11.2",
    "cli-table": "^0.3.11",
    "dotenv": "^16.0.1",
    "level": "^8.0.0",
    "multiformats": "^9.9.0",
    "picocolors": "^1.0.0",
    "pretty-bytes": "^6.0.0",
    "pretty-ms": "^8.0.0",
    "repco-common": "*",
    "repco-prisma": "*",
    "rss-parser": "^3.12.0",
    "speedometer": "^1.1.0",
    "streamx": "^2.12.5",
    "uint8arrays": "^3.1.1",
    "undici": "^5.10.0",
    "unique-names-generator": "^4.7.1",
    "vscode-uri": "^3.0.6",
    "zod": "^3.19"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.1",
    "@types/pkgjs__parseargs": "^0.10.0",
    "@types/streamx": "^2.9.1",
    "brittle": "^2.4.0",
    "esbuild": "^0.14.51",
    "get-port": "^6.1.2",
    "nanobench-utils": "^1.0.0"
  }
}
